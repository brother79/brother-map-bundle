<script src="http://api-maps.yandex.ru/2.0/?load=package.full&mode=debug&lang=ru-RU" type="text/javascript"></script>
<script type="text/javascript">
    ymaps.ready(function () {
        var map = new ymaps.Map("{{ map.htmlId }}", {
            center: [{{ map.latitude }}, {{ map.longitude }}],
            zoom:{{ map.zoom  }}, type: "{{ map.type }}",
            behaviors: ['default', 'scrollZoom']
        });
        {{ map.renderControls|raw }}
        {% if objects is defined %}
        {% for object in objects %}
           map.geoObjects.add(new ymaps.GeoObject({{ object.renderYandex|raw }}));
        {% endfor %}
        {% endif %}
    });
</script>

